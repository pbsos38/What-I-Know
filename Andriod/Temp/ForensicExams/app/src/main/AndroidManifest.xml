<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.Prince.forensicexams">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.EXTERNAL_CONTENT_URI" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_USER_DICTIONARY" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_INTERNAL_STORAGE" />

    <application
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:resizeableActivity="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:targetApi="n"
        android:allowBackup="true"
        android:fullBackupContent="true">
        <activity android:name=".Contributers" android:exported="true"/>
        <activity android:name=".CompetitionPosts" android:exported="true"/>
        <activity android:name=".TestList" android:hardwareAccelerated="true" android:exported="true"/>
        <activity android:name=".MainActivity" android:hardwareAccelerated="true" android:exported="true"/>
        <activity
            android:name=".Index"
            android:exported="true"
            android:hardwareAccelerated="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Test" android:hardwareAccelerated="true" android:exported="true"/>
        <activity android:name=".CreatePost" android:hardwareAccelerated="true" android:exported="true"/>
       <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts"
           android:exported="true"/>
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:exported="true"
            android:value="@string/admob_app_id" />
    </application>

</manifest>